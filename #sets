#sets
a={1,2,3,4,5}
b={4,5,6,7,8}
print(a)
print(b)
a.add(6)
print(a)
a.remove(2)
print(a)
c=a&b #intersection
print(c)
c=a|b #union
print(c)
c=a-b #difference
print(c)
c=b-a #difference
print(c)
{1, 2, 3, 4, 5}
{4, 5, 6, 7, 8}
{1, 2, 3, 4, 5, 6}
{1, 3, 4, 5, 6}
{4, 5, 6}
{1, 3, 4, 5, 6, 7, 8}
{1, 3}
{8, 7}
